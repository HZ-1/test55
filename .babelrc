{
  "sourceMaps": true,
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-object-rest-spread",
    ["module-resolver", {
      "root": ["./films"]
    }],
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es"
    }],
    [
      "transform-runtime",
      {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
      }
      ]
  ]
}
